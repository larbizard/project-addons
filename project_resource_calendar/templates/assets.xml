        <xpath expr=".">
            <link href="/project_resource_calendar/static/src/css/reports_style.css"
                  rel="stylesheet"/>
        </xpath>
    </template>
    <template id="assets_backend" inherit_id="web.assets_backend">
        <xpath expr=".">
            <link href="/project_resource_calendar/static/src/css/calendar_style.css"
